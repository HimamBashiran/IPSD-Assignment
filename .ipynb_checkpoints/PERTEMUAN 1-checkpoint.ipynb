   "source": [
    "def generate_sequence(n):\n",
    "    if n <= 2:\n",
    "        return [1] * n\n",
    "    seq = generate_sequence(n - 1)\n",
    "    return seq + [n * seq[-1]]\n",
    "\n",
    "# Contoh penggunaan\n",
    "n = 5\n",
    "result = generate_sequence(n)\n",
    "print(f\"Input: n = {n}\")\n",
    "print(f\"Output: {', '.join(map(str, result))}\")"
   ]
  },
